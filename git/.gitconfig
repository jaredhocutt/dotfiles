[alias]
     pl = pull --rebase
     plce = pull --commit --no-edit
     st = status
     psh = push origin master
     br = branch
     co = checkout
     lg = log --oneline --decorate
     upd = "!sh -c git checkout master; git pull --rebase; git checkout -; git rebase master;"
     p4upd = "!sh -c git checkout master; git p4 rebase; git checkout -; git rebase master;"
     p4prep = p4 submit --prepare-p4-only
[user]
	email = jaredh@netapp.com
	name = Jared Hocutt
[push]
	default = simple
[core]
	excludesfile = /Users/jaredh/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
